{
	"name": "Test Utility",
	"version": "1.0",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": [
				"https://www.corriere.it/*"
			],
			"js": [
				"loader.js"
			]
		}
	],
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"host_permissions": [
		"https://www.corriere.it/*"
	],
	"permissions": [
		"tabs",
		"activeTab",
		"scripting"
	]
}