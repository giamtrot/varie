{
	"name": "FL Utility",
	"version": "1.6",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": [
				"https://www.flickr.com/*"
			],
			"js": [
				"Mustache.js",
				"loader.js"
			],
			"css": [
				"fl.css"
			],
			"web_accessible_resources": [
				{
					"resources": [
						"template.mustache"
					],
					"matches": [
						"<all_urls>"
					]
				}
			]
		}
	],
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"host_permissions": [
		"https://www.flickr.com/*"
	],
	"permissions": [
		"tabs",
		"activeTab",
		"scripting"
	]
}