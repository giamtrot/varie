{
	"name": "FL Utility",
	"version": "1.5",
	"manifest_version": 3,
	"content_scripts": [{
		"matches": [
			"https://www.flickr.com/*"
		],
		"js": ["loader.js"]
	}],
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"host_permissions": [
		"https://www.flickr.com/*"
	],
	"permissions": [
		"tabs",
		"activeTab",
		"scripting"
	]
}